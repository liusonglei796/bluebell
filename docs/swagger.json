{
    "swagger": "2.0",
    "info": {
        "description": "Go语言实战项目——社区web框架",
        "title": "bluebell项目接口文档",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "这里写联系人姓名",
